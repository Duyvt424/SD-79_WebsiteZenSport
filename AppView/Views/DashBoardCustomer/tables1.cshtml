	<script>
		function filterByStatus() {
			var selectedStatus = $("#orderStatus").val().trim();
			console.log("Selected Status:", selectedStatus);

			// Lấy tất cả các dòng và lưu vào mảng
			var rows = $("tbody tr");
			console.log("Number of Rows:", rows.length);

			// Lặp qua mảng và so sánh trạng thái
			rows.each(function () {
				var statusCell = $(this).find("td:eq(8)");

				if (statusCell.length > 0) {
					var rowStatus = statusCell.text().trim();
					console.log("Row Status (Before):", rowStatus);

					// Chuẩn hóa giá trị trước khi so sánh
					var normalizedRowStatus = normalizeStatus(rowStatus);
					console.log("Normalized Row Status:", normalizedRowStatus);

					var shouldDisplay = selectedStatus === "-1" || normalizedRowStatus.includes(selectedStatus);
					console.log("Should Display:", shouldDisplay);

					$(this).toggle(shouldDisplay);
				}
			});
		}

		// Hàm để chuẩn hóa giá trị trạng thái
		function normalizeStatus(status) {
			// Quy đổi giá trị trạng thái theo mong muốn
			switch (status) {
				case "Chờ xác nhận":
					return "0";
				case "Đã xác nhận thông tin":
					return "1";
				case "Đã bàn giao cho ĐVVC":
					return "2";
				case "Đã nhận được hàng":
					return "3";
				case "Đã hủy đơn":
					return "4";
				default:
					return status;
			}
		}
	</script>


	<script>
		function filterByDateRange() {
			var startDateString = document.getElementById('startDate').value;
			var endDateString = document.getElementById('endDate').value;

			// Parse dates with Moment.js
			var startDate = moment(startDateString, 'YYYY-MM-DD');
			var endDate = moment(endDateString, 'YYYY-MM-DD');

			console.log("Start Date:", startDate);
			console.log("End Date:", endDate);

			if (!startDate.isValid() || !endDate.isValid()) {
				console.log("Invalid date format");
				return;
			}

			$('.table tbody tr').each(function () {
				var rowDateString = $(this).find('.date-column').text();
				console.log("Row Date String:", rowDateString);

				// Parse row date string into a valid Date object using Moment.js
				var rowDate = moment(rowDateString, 'HH:mm, DD/MM/YYYY');

				console.log("Row Date:", rowDate);

				if (rowDate.isValid() && rowDate.isSameOrAfter(startDate) && rowDate.isSameOrBefore(endDate)) {
					$(this).show();
				} else {
					$(this).hide();
				}
			});
		}


	</script>

	


