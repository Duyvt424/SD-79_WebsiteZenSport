                @Html.DisplayFor(modelItem => item.StyleID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SupplierID)
            </td>
            
            <td>
                
                @Html.ActionLink("UpdateShoesDetails", "UpdateShoesDetails", new {  id=item.ShoesDetailsId  }) |
                @Html.ActionLink("Details", "Details", new {  id=item.ShoesDetailsId  }) | 
                @Html.ActionLink("DeleteShoesDetails", "DeleteShoesDetails", new {  id=item.ShoesDetailsId  })
            </td>
        </tr>
}
    </tbody>
</table>