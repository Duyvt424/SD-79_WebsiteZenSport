										<a href="#0"> <i class="lni lni-cog"></i> Settings </a>
									</li>
									<li class="divider"></li>
									<li>
										<a href="@Url.Action("LogOut", "Employee")"> <i class="lni lni-exit"></i> Sign Out </a>
									</li>
								</ul>
							</div>
							<!-- profile end -->
						</div>
					</div>
				</div>
			</div>
		</header>
		<!-- ========== header end ========== -->
		<!-- ========== section start ========== -->
		<section class="section">
			<div class="container-fluid">
				<!-- ========== title-wrapper start ========== -->
				<div class="title-wrapper pt-30">
					<div class="row align-items-center">
						<div class="col-md-6">
							<div class="title">
								<h2>Thông tin chi tiết</h2>
							</div>
						</div>
						<!-- end col -->
						<div class="col-md-6">
							<div class="breadcrumb-wrapper">
								<nav aria-label="breadcrumb">
									<ol class="breadcrumb">
										<li class="breadcrumb-item">
											<a href="#0">Dashboard</a>
										</li>
										<li class="breadcrumb-item"><a href="#0">Quản lý nhân viên</a></li>
										<li class="breadcrumb-item active" aria-current="page">
											Thông tin chi tiết
										</li>
									</ol>
								</nav>
							</div>
						</div>
						<!-- end col -->
					</div>
					<!-- end row -->
				</div>
				<!-- ========== title-wrapper end ========== -->
				<form asp-action="settings" enctype="multipart/form-data">
					<div class="row">
						<div class="col-lg-6">
							<div class="card-style settings-card-1 mb-30">
								<div class="title mb-30 d-flex justify-content-between align-items-center">
									<h6>Thông tin cá nhân</h6>
									<button class="border-0 bg-transparent">
										<i class="lni lni-pencil-alt"></i>
									</button>
								</div>
								<div class="profile-info">
									<div class="input-style-1" style="display:none;">
										<label>ID</label>
										<input type="text" asp-for="EmployeeID" />
									</div>
									<div class="d-flex align-items-center mb-30">
										<div class="profile-image">
											<img src="@Url.Content("~/image/" + Model.Image)" alt="" />
											<div class="update-image">
												<input type="file" name="image" />
												<label for=""><i class="lni lni-cloud-upload"></i></label>
											</div>
										</div>
										<div class="profile-meta">
											<h5 class="text-bold text-dark mb-10">@Model.FullName</h5>
										</div>
									</div>
									<div class="input-style-1">
										<label>Họ và tên</label>
										<input type="text" asp-for="FullName" />
									</div>
									<div class="input-style-1">
										<label>Tên đăng hập</label>
										<input type="text" asp-for="UserName" />
									</div>
									<div class="input-style-1">
										<label>Mật khẩu</label>
										<input type="password" asp-for="Password" />
									</div>
								</div>
							</div>
							<!-- end card -->
						</div>
						<!-- end col -->

						<div class="col-lg-6">
							<div class="card-style settings-card-2 mb-30">
								<div class="title mb-30">
									<h6>Thông tin cá nhân</h6>
								</div>
								<form action="#">
									<div class="row">
										<div class="col-12">
											<div class="input-style-1">
												<label>Email</label>
												<input type="email" asp-for="Email" />
											</div>
										</div>
										<div class="col-12">
											<div class="input-style-1">
												<label>Số điện thoại</label>
												<input type="number" asp-for="PhoneNumber" />
											</div>
										</div>
										<div class="col-12">
											<div class="input-style-1">
												<label>Địa chỉ</label>
												<input type="text" asp-for="Address" />
											</div>
										</div>
										<div class="col-xxl-12" style="display:none;">
											<div class="select-style-1">
												<label>Vai trò</label>
												<div class="select-position">
													<select asp-for="RoleID" class="light-bg" asp-items="@ViewBag.RoleList"></select>
												</div>
											</div>
										</div>
										<div class="col-xxl-12">
											<div class="select-style-1">
												<label>Giới tính</label>
												<div class="select-position">
													<select asp-for="Sex" class="light-bg">
														<option value="0">Nam</option>
														<option value="1">Nữ</option>
														<option value="2">Không xác định</option>
													</select>
												</div>
											</div>
										</div>
										<div class="col-12">
											<div class="input-style-1">
												<label>Mô tả</label>
												<textarea placeholder="Type here" rows="4" asp-for="IdentificationCode"></textarea>
											</div>
										</div>
										<div class="col-12">
											<button type="submit" class="main-btn primary-btn btn-hover">
												Cập nhật thông tin
											</button>
										</div>
									</div>
								</form>
							</div>
							<!-- end card -->
						</div>
						<!-- end col -->
					</div>
				</form>
				<!-- end row -->
			</div>
			<!-- end container -->
		</section>
	</main>
	<script src="~/assets/js/bootstrap.bundle.min.js"></script>
	<script src="~/assets/js/Chart.min.js"></script>
	<script src="~/assets/js/dynamic-pie-chart.js"></script>
	<script src="~/assets/js/moment.min.js"></script>
	<script src="~/assets/js/fullcalendar.js"></script>
	<script src="~/assets/js/jvectormap.min.js"></script>
	<script src="~/assets/js/world-merc.js"></script>
	<script src="~/assets/js/polyfill.js"></script>
	<script src="~/assets/js/main.js"></script>
</body>
</html>
