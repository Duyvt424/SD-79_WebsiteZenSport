					<!-- end col-->
					<div class="col-md-6">
						<div class="terms d-flex justify-content-center justify-content-md-end">
						</div>
					</div>
				</div>
				<!-- end row -->
			</div>
			<!-- end container -->
		</footer>
		<!-- ========== footer end =========== -->
	</main>
	<script>
		var dateEl = document.getElementById('date');
