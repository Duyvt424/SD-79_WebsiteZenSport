											$('#resultContainer tr').hide();

											// Hiển thị chỉ 10 sản phẩm ban đầu
											$('#resultContainer tr').slice(0, recordsPerPage).show();

											// Khởi tạo phân trang
											$('#pagination').twbsPagination({
